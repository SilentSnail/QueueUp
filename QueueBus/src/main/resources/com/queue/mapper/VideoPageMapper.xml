<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.queue.mapper.VideoPageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.queue.entity.VideoPage">
        <id column="IDENTITY" property="identity" />
        <result column="TITLE" property="title" />
        <result column="PAGE_URL" property="pageUrl" />
        <result column="VIDEO_ID" property="videoId" />
        <result column="SOURCE_ID" property="sourceId" />
        <result column="STATUS" property="status" />
        <result column="CREATE_TIME" property="createTime" />
        <result column="CREATE_BY" property="createBy" />
        <result column="UPDATE_TIME" property="updateTime" />
        <result column="UPDATE_BY" property="updateBy" />
        <result column="EVALUATE" property="evaluate" />
        <result column="REMARK" property="remark" />
        <result column="IS_VIP" property="isVip" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        IDENTITY, TITLE, PAGE_URL, VIDEO_ID, SOURCE_ID, STATUS, CREATE_TIME, CREATE_BY, UPDATE_TIME, UPDATE_BY, EVALUATE, REMARK, IS_VIP
    </sql>

</mapper>
